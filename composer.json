{
    "name": "alt-three/throttle",
    "description": "A request rate limiter for Laravel 5.2+",
    "keywords": ["throttle", "http", "throttle", "rate limit", "rate limiter", "Throttle", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/cache": "5.5.*|5.6.*",
        "illuminate/http": "5.5.*|5.6.*"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.1",
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Throttle\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
